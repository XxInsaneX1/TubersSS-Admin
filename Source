local function notif(str,dur)
	game:FindService("StarterGui"):SetCore("SendNotification", {
		Title = "Tss",
		Text = str,
		Icon = "",
		Duration = dur or 1
	})
end
notif('Loading assets...',3)
wait(3)
notif('Unable to load assets. Error code: 266.',3)
wait(3)
notif('Tss is still in development! It is not ready, this is why you are recieving this message.',10)
end)
